{
    "manifest_version": 2,
    "name": "News_Feed",
    "description": "Get yourself updated every time you open a new tab.",
    "version": "1",
    "author": "Arpan Godiyal",
   "browser_action": {
      "default_title": "Have a good day"
    },
   "chrome_url_overrides" : {
     "newtab": "index.html"
   },
    "permissions": ["activeTab"]
}